{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/gpio/**",
                "${workspaceFolder}/build/_deps/googletest-src/googletest/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "windows-gcc-arm",
            "configurationProvider": "ms-vscode.cpptools" // https://stackoverflow.com/questions/45583473/include-errors-detected-in-vscode
        }
    ],
    "version": 4
}